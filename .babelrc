{
  "presets": [
    [
      "@babel/preset-env",
      {
        "targets": {
          "chrome": "88"
        }
      }
    ],
    "@babel/preset-react",
    [
      "@babel/preset-typescript",
      {
        "tsconfig": "./view/tsconfig.json"
      }
    ]
  ],

  "plugins": [
    [
      "@babel/plugin-transform-runtime",
      {
        "corejs": 3,
        "helpers": true,
        "useESModules": true
      }
    ],
    ["@babel/plugin-proposal-class-properties", { "loose": false }]
  ]
}
